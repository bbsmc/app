  "query": "\n        SELECT id, user_id, ban_type, reason, internal_reason, banned_by,\n               banned_at, expires_at, is_active, metadata,\n               COUNT(*) OVER() as total_count\n        FROM user_bans\n        WHERE is_active = $1\n        ORDER BY banned_at DESC\n        LIMIT $2 OFFSET $3\n        ",
